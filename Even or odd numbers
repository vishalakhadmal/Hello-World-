#include<stdio.h>
#include<conio.h>
void main()
{
    int num;
    printf("\n\n For Checking given number is Even or Odd");
    printf("\n\n Enter any Number = ");
    scanf("%d",&num);
    
    if ( num %2 == 0 )
       printf("\n\n %d is Even", num );
    else
       printf("\n\n %d is Odd", num );
       
}






